package ex02_value_ref;

public class MemberBean {
	private String name;
	private int age;
	private String message;
	
	
	public void output() {
		System.out.println("이름 :"+name+"["+"나이 :"+age+"]"+message);
	}


	//--------------------------------
	//생성자		
	public MemberBean(String name, int age, String message) {
		super();
		this.name = name;
		this.age = age;
		this.message = message;
	}
	
	
	
	
	
}
